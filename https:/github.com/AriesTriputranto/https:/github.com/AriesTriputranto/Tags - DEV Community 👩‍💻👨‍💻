  .l\:inline-block { display: inline-block; }
  .l\:flex { display: flex; }
  .l\:inline-flex { display: inline-flex; }
  .l\:grid { display: grid; }
  .l\:visible { visibility: visible; }
  .l\:invisible { visibility: hidden; }
  .l\:static { position: static; }
  .l\:absolute { position: absolute; }
  .l\:fixed { position: fixed; }
  .l\:sticky { position: sticky; }
  .l\:relative { position: relative; }
  .l\:break-normal { word-break: normal; }
  .l\:break-all { word-break: break-all; }
